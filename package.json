  },
  "dependencies": {
    "config": "^3.3.3",
    "mysql2": "^2.2.5",
    "node-fetch": "^2.6.1",
    "short-uuid": "^4.1.0",
    "slow-json-stringify": "^2.0.0",
    "stream-json": "^1.7.1",
    "uuid": "^8.3.1"
  }
